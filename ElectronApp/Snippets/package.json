{
  "name": "snippets",
  "productName": "snippets",
  "version": "1.0.0",
  "description": "Electron app to store all kinds of snippets",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Andreas Roither",
    "email": "andi.roither@protonmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "snippets"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                },
                {
                  "html": "./src/login.html",
                  "js": "./src/login.ts",
                  "name": "login_window"
                },
                {
                  "html": "./src/splash.html",
                  "js": "./src/splash.ts",
                  "name": "splash_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "electron": "8.2.5",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-select": "^1.13.17",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "popper.js": "^1.16.1"
  }
}
